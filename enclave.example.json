{
 "exe": "compass-oracle",
 "key": "private.pem",
 "debug": true,
 "heapSize": 1024,
 "executableHeap": false,
 "productID": 1234,
 "securityVersion": 2,
 "mounts": [
    {
            "source": "/path/to/your/compass",
            "target": "/compass",
            "type": "hostfs",
            "readOnly": false
    }
 ],
 "env": [
    {
            "name": "PWD",
            "value": "/compass"
    }
 ],
 "files": [
    {
            "source": "/etc/ssl/certs/ca-certificates.crt",
            "target": "/etc/ssl/certs/ca-certificates.crt"
    }
 ]
}
